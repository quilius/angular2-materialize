{
  "name": "angular2-materialize",
  "version": "15.14.0",
  "description": "Angular 2 support for Materialize CSS framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/InfomediaLtd/angular2-materialize.git"
  },
  "homepage": "https://github.com/InfomediaLtd/angular2-materialize#readme",
  "bugs": {
    "url": "https://github.com/InfomediaLtd/angular2-materialize/issues"
  },
  "files": [
    "lib_commonjs",
    "lib_esmodule",
    "README",
    "LICENSE"
  ],
  "author": "Ruby Boyarski <rubyboy@gmail.com>",
  "license": "MIT",
  "keywords": [
    "angular 14",
    "materialize",
    "materializecss",
    "materialize-css",
    "materialize css"
  ],
  "main": "./lib_commonjs/index.js",
  "module": "./lib_esmodule/index.js",
  "jsnext:main": "./lib_esmodule/index.js",
  "typings": "./lib_commonjs/index.d.ts",
  "scripts": {
    "build:commonjs": "rimraf lib_commonjs && ngc -p tsconfig.json",
    "build:esmodule": "rimraf lib_esmodule && ngc -p tsconfig.module.json",
    "build": "npm run build:commonjs && npm run build:esmodule",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@angular/common": ">=14.0.0",
    "@angular/core": ">=14.0.0"
  },
  "devDependencies": {
    "@angular/animations": "^14.2.6",
    "@angular/cli": "^14.2.6",
    "@angular/common": "^14.2.6",
    "@angular/compiler": "^14.2.6",
    "@angular/compiler-cli": "^14.2.6",
    "@angular/core": "^14.2.6",
    "@angular/forms": "^14.2.6",
    "@angular/platform-browser": "^14.2.6",
    "@angular/platform-browser-dynamic": "^14.2.6",
    "@angular/router": "^14.2.6",
    "commitizen": "4.2.5",
    "core-js": "^3.25.5",
    "cz-conventional-changelog": "3.3.0",
    "ghooks": "2.0.4",
    "gulp": "4.0.2",
    "gulp-rimraf": "1.0.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.1",
    "materialize-css": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "run-sequence": "2.2.1",
    "rxjs": "^7.5.7",
    "semantic-release": "^19.0.5",
    "tsickle": "^0.46.3",
    "typescript": "^4.7.2",
